import java.io.*;
import java.util.*;
class UserMainCode {
    public int seriesN(int input1, int input2, int input3, int input4) {
        int a = input1;
        int b = input2;
        int c = input3;
        int N = input4;
        if (N == 1) return a;
        if (N == 2) return b;
        if (N == 3) return c;
        int gap1 = b - a;
        int gap2 = c - b;
        int value = c;
        for (int i = 4; i <= N; i++) {
            if ((i - 4) % 2 == 0)
                value += gap1;
            else
                value += gap2;
        }
        return value;
    }
}
